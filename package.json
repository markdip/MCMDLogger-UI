{
  "name": "mcmdgui",
  "version": "1.0.1",
  "description": "Modern GUI for MCmdLogger - MITM tool for Minecraft",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --enable-logging",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.mcmdlogger.gui",
    "win": {
      "target": "portable"
    },
    "extraResources": [
      "mcmdlogger-neo.exe",
      "scripts-templates/**",
      "scripts-catalog.json"
    ]
  },
  "keywords": [
    "minecraft",
    "mcmdlogger",
    "gui",
    "electron"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "discord.js": "^14.25.1",
    "node-telegram-bot-api": "^0.66.0"
  }
}